<mvc:View xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" controllerName="sources.controller.Users">
	<Shell appWidthLimited="false">
		<App class="sapUiResponsiveMargin" width="auto"  xmlns:f="sap.ui.layout.form">
			<pages>
				<Page title="Users">
					<content>
						<VBox fitContainer="true">
							<Table
									id="userTable"
									inset="false"
									items="{
						path: '/data'
					}"
									class="sapFDynamicPageAlignContent"
									width="auto">
								<columns>
									<Column width="12em">
										<Text text="SocketId"/>
									</Column>
									<Column>
										<Text text="UserId"/>
									</Column>
									<Column>
										<Text text="Csoport"/>
									</Column>
								</columns>
								<items>
									<ColumnListItem type="Navigation" press=".onListItemPress">
										<cells>
											<ObjectIdentifier title="{SOCKETID}" text="{products>ProductId}"/>
											<Label text="{userid}"/>
											<Label text="{csoport}"/>
										</cells>
									</ColumnListItem>
								</items>
							</Table>
						</VBox>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>